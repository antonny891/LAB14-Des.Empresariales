{
  "version": 2,
  "builds": [
    {
      "src": "**/*.csproj",
      "use": "@vercel/node@2.x",
      "config": {
        "includeFiles": "out/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/$1"
    }
  ]
}


